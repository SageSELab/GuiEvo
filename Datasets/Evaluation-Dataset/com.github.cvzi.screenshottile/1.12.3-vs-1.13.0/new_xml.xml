content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[34,537][1046,1439]"><node index="0" text="" resource-id="" class="android.widget.LinearLayout" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,563][1020,634]"><node index="0" text="ON" resource-id="com.github.cvzi.screenshottile:id/switchLegacy" class="android.widget.Switch" package="com.github.cvzi.screenshottile" content-desc="Legacy Method" checkable="true" checked="true" clickable="true" enabled="false" focusable="true" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,569][182,634]" /><node index="1" text="Legacy Method" resource-id="com.github.cvzi.screenshottile:id/textTitleLegacy" class="android.widget.TextView" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[182,563][546,634]" /></node><node index="1" text="The legacy method to take screenshots uses the screen recording features of Android&#10;When you take a screenshot, no sound is played. A silent notification is shown to open, share or edit the screenshot. The notification can be disabled in the app settings.&#10;Before you can take a screenshot you need to add the tile to the quick settings, accept the permission to record the screen and the permission to access the internal storage to save the image file.&#10;See the tutorial on how to do it.&#10;&#10;On some phones this method does not work, especially on newer Androids (9.0+) it is unstable and it may ask for permission on every single screenshot. If that happens you should use the native method below." resource-id="com.github.cvzi.screenshottile:id/textDescLegacy" class="android.widget.TextView" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,634][1020,1287]" /><node index="2" text="Tutorial" resource-id="com.github.cvzi.screenshottile:id/buttonTutorial" class="android.widget.Button" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="true" enabled="true" focusable="true" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,1287][281,1413]" /></node></node><node index="2" text="" resource-id="" class="android.widget.FrameLayout" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[34,1481][1046,1794]"><node index="0" text="" resource-id="com.github.cvzi.screenshottile:id/linearLayoutNative" class="android.widget.LinearLayout" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[34,1481][1046,1794]"><node index="0" text="" resource-id="" class="android.widget.LinearLayout" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,1507][1020,1578]"><node index="0" text="OFF" resource-id="com.github.cvzi.screenshottile:id/switchNative" class="android.widget.Switch" package="com.github.cvzi.screenshottile" content-desc="Native Method" checkable="true" checked="false" clickable="true" enabled="false" focusable="true" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,1513][182,1578]" /><node index="1" text="Native Method" resource-id="com.github.cvzi.screenshottile:id/textTitleNative" class="android.widget.TextView" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[182,1507][529,1578]" /></node><node index="1" text="&#128683; Requires Android 9 Pie" resource-id="" class="android.widget.TextView" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[60,1578][1020,1629]" /><node index="2" text="The native method to take screenshots simulates a press of Home+Power button and uses the screenshot method of your device.&#10;When you take a screenshot, it may play a sound or show a screenshot editor. Screenshots are stored in the standard folder for pictures. This depends on your phone and cannot be changed by this app. You might be able to change the behavior in your phone's settings.&#10;&#10;Before you can take a screenshot you need to enable the accessibility service in the phone settings. Press the button below to open the accessibility settings." resource-id="com.github.cvzi.screenshottile:id/textDescNative" class="android.widget.TextView" package="com.github.cvzi.screenshottile" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="f